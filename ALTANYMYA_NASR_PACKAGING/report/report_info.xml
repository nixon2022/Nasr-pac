        <field name="report_name">ALTANYMYA_NASR_PACKAGING.nasr_report_id</field>
        <field name="report_file">ALTANYMYA_NASR_PACKAGING.nasr_report_id</field>
        <field name="binding_model_id" ref="model_mrp_production"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="ALTANYMYA_NASR_PACKAGING.paperformat_A3_label_sheet"/>
    </record>

