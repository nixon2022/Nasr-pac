<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_account_move_inherited" model="ir.ui.view">
        <field name="name">account.move.inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <field name="journal_id" position="after">
                <field name="total_price_words" invisible="1"/>
                <field name="total_price_words_arabic" invisible="1"/>
                <field name="Total_quantity_ordered" invisible="1"/>
            </field>
            <field name="invoice_origin" position="after">
                <field name="delivery_ids" widget="many2many_tags" invisible="0" readonly="1"/>
            </field>
            <xpath expr="//page[@id='other_tab_entry'][1]" position="after">
                <page string="Bank Details" name="bank_details_page">
                    <group>
                        <field name="bank_details"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>